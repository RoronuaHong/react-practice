{
    "presets": [
        "react",
        "stage-0",
        ["env", {
            "module": false,
            "browser": ["last 2 versions", "ie >= 9"]
        }]
    ],
    "plugins": [
        "transform-decorators-legacy",
        "transform-runtime",
        "react-hot-loader/babel",
		["transform-class-properties", { "spec": true }]
    ]
}